{
  "name": "stonks",
  "version": "0.0.1",
  "description": "See what's up with your stonks",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "webpack": "webpack --watch --mode=development",
    "postinstall": "webpack",
    "build": "webpack",
    "start": "webpack serve --open",
    "format": "prettier --write \"**/**/**/*.+(js|json|css|html)\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nverno/stonks.git"
  },
  "keywords": [
    "stonks",
    "stocks"
  ],
  "author": "Noah Peart <noah.v.peart@gmail.com> (https://www.github.com/nverno)",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/nverno/stonks/issues"
  },
  "homepage": "https://github.com/nverno/stonks#readme",
  "engines": {
    "node": ">=15",
    "npm": ">=6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "codecov": "^3.8.1",
    "css-loader": "^5.0.1",
    "ejs-loader": "^0.5.0",
    "gh-pages": "^3.1.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "sass": "^1.32.2",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1"
  },
  "lint-staged": {
    "**/**/**/*.(js|jsx|md|css|html)": [
      "prettier --trailing-comma es5 --single-quote --write"
    ],
    "**/*.json": [
      "prettier --write"
    ],
    "yarn.lock": [
      "git rm --cached"
    ],
    "package-lock.json": [
      "git rm --cached"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"[Husky] pre-commit\" && lint-staged"
    }
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "ejs": "^3.1.5",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "collectCoverage": true
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/no-anonymous-default-export": "off"
    }
  }
}
