{
  "name": "stonks",
  "version": "0.0.1",
  "description": "See what's up with your stonks",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "format": "prettier --write \"**/*.+(js|json|css)\"",
    "webpack": "webpack --watch --mode=development",
    "postinstall": "webpack",
    "build": "webpack",
    "start": "node build/bundle.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "engines": {
    "node": ">=15",
    "npm": ">=6"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/no-anonymous-default-export": "off"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nverno/stonks.git"
  },
  "keywords": [
    "stonks",
    "stocks"
  ],
  "author": "Noah Peart <noah.v.peart@gmail.com> (https://www.github.com/nverno)",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/nverno/stonks/issues"
  },
  "homepage": "https://github.com/nverno/stonks#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "codecov": "^3.8.1",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "collectCoverage": true
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "lint-staged": {
    "**/**/*.(js|jsx|md|css|html)": [
      "prettier --trailing-comma es5 --single-quote --write"
    ],
    "**/*.json": [
      "prettier --write"
    ],
    "yarn.lock": [
      "git rm --cached"
    ],
    "package-lock.json": [
      "git rm --cached"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"[Husky] pre-commit\" && lint-staged"
    }
  }
}
