{
  "name": "stonks",
  "version": "0.0.1",
  "description": "See what's up with your stonks",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "build": "npm run build --prefix stonks-panel",
    "format": "prettier --write \"**/**/**/*.+(js|json|css)\"",
    "start": "npm start --prefix stonks-panel",
    "predeploy": "npm run build --prefix stonks-panel",
    "deploy": "gh-pages -d stonks-panel/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nverno/stonks.git"
  },
  "keywords": [
    "stonks",
    "stocks"
  ],
  "author": "Noah Peart <noah.v.peart@gmail.com> (https://www.github.com/nverno)",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/nverno/stonks/issues"
  },
  "homepage": "https://github.com/nverno/stonks#readme",
  "engines": {
    "node": ">=15",
    "npm": ">=6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "babel-loader": "^8.2.2",
    "codecov": "^3.8.1",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "lint-staged": {
    "**/**/**/*.(js|jsx|md|css|html)": [
      "prettier --trailing-comma es5 --single-quote --write"
    ],
    "**/*.json": [
      "prettier --write"
    ],
    "yarn.lock": [
      "git rm --cached"
    ],
    "package-lock.json": [
      "git rm --cached"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"[Husky] pre-commit\" && lint-staged"
    }
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "collectCoverage": true
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/no-anonymous-default-export": "off"
    }
  }
}
